<section class="b-contacts s-shadow" style="padding-bottom: 40px !important;">
  <div class="container">
    <div class="row">
      <div class="col-xs-12">
        <div class="b-contacts__form">
          <header class="b-contacts__form-header s-lineDownLeft wow zoomInUp" data-wow-delay="0.5s">
            <h2 class="s-titleDet">Sign In</h2> 
          </header>
          <p class=" wow zoomInUp" data-wow-delay="0.5s">Enter your valid username and password</p>
          <div id="success"></div>
          <form id="contactForm" onsubmit="return false;" novalidate class="s-form wow zoomInUp" data-wow-delay="0.5s">
            <input type="text" placeholder="USERNAME" value="" name="user-name" id="user-name" />
            <input type="text" placeholder="PASSWORD" value="" name="password" id="password" />
            <button type="submit" class="btn m-btn">SIGN IN<span class="fa fa-angle-right"></span></button>
          </form>
        </div>
      </div>
    </div>
    <br/> 
    <div class="row">
      <div class="col-xs-12">
        <p class=" wow zoomInUp" data-wow-delay="0.5s">Not yet a member? Click <a href="/profile/register">here</a> to register</p>
        <p class=" wow zoomInUp" data-wow-delay="0.5s">Forgot your password? Click <a href="/profile/forgotpassword">here</a> to reset your password</p> 
      </div>
    </div>
  </div>
</section><!--b-contacts-->